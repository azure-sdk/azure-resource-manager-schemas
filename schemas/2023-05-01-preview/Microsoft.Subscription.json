{
  "id": "https://schema.management.azure.com/schemas/2023-05-01-preview/Microsoft.Subscription.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Microsoft.Subscription",
  "description": "Microsoft Subscription Resource Types",
  "resourceDefinitions": {},
  "tenant_resourceDefinitions": {
    "policies": {
      "type": "object",
      "properties": {
        "apiVersion": {
          "type": "string",
          "enum": [
            "2023-05-01-preview"
          ]
        },
        "name": {
          "type": "string",
          "enum": [
            "default"
          ]
        },
        "properties": {
          "oneOf": [
            {
              "$ref": "#/definitions/PutTenantPolicyRequestProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Put tenant policy request properties."
        },
        "type": {
          "type": "string",
          "enum": [
            "Microsoft.Subscription/policies"
          ]
        }
      },
      "required": [
        "apiVersion",
        "name",
        "properties",
        "type"
      ],
      "description": "Microsoft.Subscription/policies"
    }
  },
  "definitions": {
    "PutTenantPolicyRequestProperties": {
      "type": "object",
      "properties": {
        "exemptedPrincipals": {
          "oneOf": [
            {
              "type": "array",
              "items": {
                "type": "string",
                "pattern": "^[0-9a-fA-F]{8}(-[0-9a-fA-F]{4}){3}-[0-9a-fA-F]{12}$"
              }
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "List of user objectIds that are exempted from the set subscription tenant policies for the user's tenant."
        },
        "subscriptionsEnteringTenant": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Allowed",
                "NotAllowed"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "subscriptionsLeavingTenant": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Allowed",
                "NotAllowed"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        }
      },
      "description": "Put tenant policy request properties."
    }
  }
}