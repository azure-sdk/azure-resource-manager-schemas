  "definitions": {
    "ClusterProperties": {
      "type": "object",
      "properties": {
        "minTlsVersion": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "1.0",
                "1.1",
                "1.2"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "The minimum TLS version for the cluster to support, e.g. the default of '1.2'."
        }
      },
      "description": "Properties of a RedisEnterprise cluster"
    },
    "DatabasePropertiesCreateOrUpdate": {
      "type": "object",
      "properties": {
        "clientProtocol": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "Encrypted",
                "Plaintext"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Specifies whether redis clients can connect using TLS-encrypted or plaintext redis protocols. Default is TLS-encrypted."
        },
        "evictionPolicy": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "AllKeysLFU",
                "AllKeysLRU",
                "AllKeysRandom",
                "VolatileLRU",
                "VolatileLFU",
                "VolatileTTL",
                "VolatileRandom",
                "NoEviction"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Redis eviction policy - default is VolatileLRU."
        },
        "persistence": {
          "oneOf": [
            {
              "$ref": "#/definitions/Persistence"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Persistence-related configuration for the RedisEnterprise database"
        },
        "port": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Port number for the database to listen on. E.g. 10000."
        }
      },
      "required": [
        "persistence"
      ],
      "description": "Properties of a Database on a RedisEnterprise cluster"
    },
    "Persistence": {
      "type": "object",
      "properties": {
        "aofEnabled": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Sets whether AOF is enabled. Defaults to false."
        },
        "aofFrequency": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "1s",
                "always"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Sets the frequency at which data is written to disk."
        },
        "rdbEnabled": {
          "oneOf": [
            {
              "type": "boolean"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Sets whether RDB is enabled. Defaults to false."
        },
        "rdbFrequency": {
          "oneOf": [
            {
              "type": "string",
              "enum": [
                "1h",
                "6h",
                "12h"
              ]
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ],
          "description": "Sets the frequency at which a snapshot of the database is created."
        }
      },
      "required": [
        "aofEnabled",
        "rdbEnabled"
      ],
      "description": "Persistence-related configuration for the RedisEnterprise database"
    },
    "PrivateEndpoint": {
      "type": "object",
