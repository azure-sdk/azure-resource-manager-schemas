{
  "id": "https://schema.management.azure.com/schemas/2025-10-03-preview/Microsoft.Monitor.Accounts.json#",
  "title": "Microsoft.Monitor",
  "description": "Microsoft Monitor Resource Types",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "resourceDefinitions": {
    "accounts": {
      "description": "Microsoft.Monitor/accounts",
      "properties": {
        "apiVersion": {
          "enum": [
            "2025-10-03-preview"
          ],
          "type": "string"
        },
        "location": {
          "description": "The geo-location where the resource lives",
          "type": "string"
        },
        "name": {
          "description": "The name of the Azure Monitor Workspace. The name is case insensitive",
          "oneOf": [
            {
              "pattern": "^(?!-)[a-zA-Z0-9-]+[^-]$",
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "properties": {
          "description": "Resource properties",
          "oneOf": [
            {
              "$ref": "#/definitions/AzureMonitorWorkspace"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "resources": {
          "items": {
            "oneOf": [
              {
                "$ref": "#/definitions/accounts_issues_childResource"
              },
              {
                "$ref": "#/definitions/accounts_metricsContainers_childResource"
              }
            ]
          },
          "type": "array"
        },
        "tags": {
          "description": "Resource tags.",
          "oneOf": [
            {
              "additionalProperties": {
                "type": "string"
              },
              "properties": {},
              "type": "object"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "Microsoft.Monitor/accounts"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    },
    "accounts_issues": {
      "description": "Microsoft.Monitor/accounts/issues",
      "properties": {
        "apiVersion": {
          "enum": [
            "2025-10-03-preview"
          ],
          "type": "string"
        },
        "name": {
          "description": "The name of the IssueResource",
          "oneOf": [
            {
              "pattern": "^(?!-)[a-zA-Z0-9-]+[^-]$",
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "properties": {
          "description": "The resource-specific properties for this resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/IssueProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "Microsoft.Monitor/accounts/issues"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    },
    "accounts_metricsContainers": {
      "description": "Microsoft.Monitor/accounts/metricsContainers",
      "properties": {
        "apiVersion": {
          "enum": [
            "2025-10-03-preview"
          ],
          "type": "string"
        },
        "name": {
          "description": "The name of the MetricsContainer",
          "oneOf": [
            {
              "pattern": "^default$",
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "properties": {
          "description": "The resource-specific properties for this resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/MetricsContainer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "Microsoft.Monitor/accounts/metricsContainers"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    }
  },
  "definitions": {
    "AzureMonitorWorkspace": {
      "description": "Properties of an Azure Monitor Workspace",
      "properties": {
        "publicNetworkAccess": {
          "description": "Gets or sets allow or disallow public network access to Azure Monitor Workspace",
          "oneOf": [
            {
              "enum": [
                "Enabled",
                "Disabled"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        }
      },
      "type": "object"
    },
    "Background": {
      "description": "The issue background information",
      "properties": {
        "details": {
          "description": "The background details",
          "oneOf": [
            {
              "items": {
                "$ref": "#/definitions/BackgroundDetails"
              },
              "type": "array"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "text": {
          "description": "The background text",
          "type": "string"
        },
        "type": {
          "description": "The background type",
          "type": "string"
        }
      },
      "type": "object"
    },
    "BackgroundDetails": {
      "description": "A background details element",
      "properties": {
        "name": {
          "description": "The background details name",
          "type": "string"
        },
        "value": {
          "description": "The background details value",
          "type": "string"
        }
      },
      "required": [
        "name",
        "value"
      ],
      "type": "object"
    },
    "IssueProperties": {
      "description": "The issue properties",
      "properties": {
        "background": {
          "description": "The issue background information",
          "oneOf": [
            {
              "$ref": "#/definitions/Background"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "impactTime": {
          "description": "The issue impact time (in UTC)",
          "format": "date-time",
          "type": "string"
        },
        "severity": {
          "description": "The issue severity",
          "type": "string"
        },
        "status": {
          "description": "The issue status",
          "oneOf": [
            {
              "enum": [
                "New",
                "InProgress",
                "Mitigated",
                "Closed",
                "Canceled"
              ],
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "title": {
          "description": "The issue title",
          "type": "string"
        }
      },
      "required": [
        "title",
        "status",
        "severity",
        "impactTime"
      ],
      "type": "object"
    },
    "MetricsContainer": {
      "description": "Properties of a metrics container.",
      "properties": {
        "limits": {
          "description": "Metrics limits.",
          "oneOf": [
            {
              "$ref": "#/definitions/MetricsLimits"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "version": {
          "description": "The version of Metrics Query Service that this AMW will use for all metric queries.",
          "type": "string"
        }
      },
      "type": "object"
    },
    "MetricsLimits": {
      "description": "Definition of Metrics limits",
      "properties": {
        "maxActiveTimeSeries": {
          "description": "Maximum number of active time series allowed on the metrics containers.",
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "maxEventsPerMinute": {
          "description": "Maximum rate (events per minute) that can be processed on the metrics containers.",
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        }
      },
      "type": "object"
    },
    "accounts_issues_childResource": {
      "description": "Microsoft.Monitor/accounts/issues",
      "properties": {
        "apiVersion": {
          "enum": [
            "2025-10-03-preview"
          ],
          "type": "string"
        },
        "name": {
          "description": "The name of the IssueResource",
          "oneOf": [
            {
              "pattern": "^(?!-)[a-zA-Z0-9-]+[^-]$",
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "properties": {
          "description": "The resource-specific properties for this resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/IssueProperties"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "issues"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    },
    "accounts_metricsContainers_childResource": {
      "description": "Microsoft.Monitor/accounts/metricsContainers",
      "properties": {
        "apiVersion": {
          "enum": [
            "2025-10-03-preview"
          ],
          "type": "string"
        },
        "name": {
          "description": "The name of the MetricsContainer",
          "oneOf": [
            {
              "pattern": "^default$",
              "type": "string"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "properties": {
          "description": "The resource-specific properties for this resource.",
          "oneOf": [
            {
              "$ref": "#/definitions/MetricsContainer"
            },
            {
              "$ref": "https://schema.management.azure.com/schemas/common/definitions.json#/definitions/expression"
            }
          ]
        },
        "type": {
          "enum": [
            "metricsContainers"
          ],
          "type": "string"
        }
      },
      "required": [
        "name",
        "properties",
        "apiVersion",
        "type"
      ],
      "type": "object"
    }
  }
}